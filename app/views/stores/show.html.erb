<p style="color: green">
  <%= notice %>
</p>

<%= render @store %>

<div class="btn-group">
  <%= link_to "Edit this store", edit_store_path(@store), class: 'btn btn-sm btn-outline-warning' %>
  <%= link_to "Back to stores", stores_path, class: 'btn btn-sm btn-outline-secondary' %>
  <%= button_to "Destroy this store", @store, method: :delete, class: 'btn btn-sm btn-outline-danger' %>
</div>
